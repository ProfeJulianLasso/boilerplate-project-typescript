{
  "name": "boilerplate-project-typescript",
  "version": "1.0.0",
  "description": "Configuraci√≥n base para iniciar un proyecto con typescript",
  "main": "app.js",
  "scripts": {
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node-esm' ./src/app.ts",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "fix": "eslint --fix 'src/**/*.ts' 'tests/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "prepare": "husky install",
    "dup": "jscpd --pattern 'src/**/*.ts' --reporters 'consoleFull,console,html' --mode 'strict' --min-lines 5 --min-tokens 70 --output './report'"
  },
  "keywords": [],
  "author": "Asdrubal Hernandez",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jscpd": "^3.5.9",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
